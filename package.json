{
  "name": "hms-web",
  "version": "0.0.0",
   "description": "Hims platform",
  "author": "Jaya krishna Lakkoju",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "make-exe": "ng build --configuration desktop && electron-builder",
    "jay": "ng build && git add . && git commit -m \"Latest hims  update\" && git push origin main"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.7.0",
  "build": {
    "appId": "com.hms.karthihims",
    "productName": "HMS",
    "directories": {
      "output": "dist/desktop-pkg"
    },
    "win": {
      "target": "portable"
    },
    "files": [
      "dist/hms-web/**/*",
      "main.js",
      "package.json"
    ]
  },
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/postcss": "^4.1.18",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
